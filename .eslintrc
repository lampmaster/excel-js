{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "indent": ["error", 2],
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": "off",
    "operator-linebreak": "off"
  },
  "extends": ["eslint:recommended", "google"],
}
