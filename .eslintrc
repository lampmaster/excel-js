{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "indent": "off",
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "no-trailing-spaces": "off",
    "operator-linebreak": "off",
    "no-unused-vars": "off",
    "no-debugger": "off",
    "linebreak-style": ["error", "unix"]
  },
  "extends": ["eslint:recommended", "google"]
}
